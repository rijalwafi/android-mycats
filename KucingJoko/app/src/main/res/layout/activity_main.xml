<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
 <RelativeLayout
     android:layout_marginTop="30dp"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     >
     <ImageView
         android:layout_below="@+id/imageBeranda"
         android:src="@drawable/circle"
         android:layout_width="100dp"
         android:layout_marginTop="-30dp"
         android:elevation="-10dp"
         android:layout_height="100dp"/>
     <ImageView
         android:layout_alignParentBottom="true"
         android:layout_alignParentRight="true"
         android:src="@drawable/circle"
         android:layout_width="100dp"
         android:layout_marginRight="-20dp"
         android:layout_marginTop="-30dp"
         android:elevation="-10dp"
         android:layout_height="100dp"/>
     <ImageView
         android:layout_alignParentTop="true"
         android:src="@drawable/circle"
         android:layout_width="100dp"
         android:layout_marginRight="-20dp"
         android:layout_marginTop="-30dp"
         android:elevation="-10dp"
         android:layout_height="100dp"/>
     <ImageView

         android:layout_alignParentRight="true"
         android:src="@drawable/circle"
         android:layout_width="100dp"

         android:layout_marginRight="-20dp"
         android:layout_marginTop="100dp"
         android:elevation="-10dp"
         android:layout_height="100dp"/>
  <ImageView
      android:id="@+id/imageBeranda"
      android:layout_marginTop="100dp"
      android:elevation="-20dp"
      android:src="@drawable/img_logo_login"
      android:layout_width="wrap_content"
      android:layout_height="400dp"/>
  <RelativeLayout
      android:layout_margin="22dp"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">
  <LinearLayout
      android:id="@+id/linearHome1"
      android:orientation="vertical"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
  <androidx.cardview.widget.CardView
      android:id="@+id/cv_catList"
      app:cardUseCompatPadding="true"
      app:cardCornerRadius="5dp"
      app:cardPreventCornerOverlap="true"
      app:cardElevation="5dp"
      android:layout_width="150dp"
      android:layout_height="150dp">

   <ImageView
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:src="@drawable/tombol_a"/>

  </androidx.cardview.widget.CardView>
   <TextView
       android:textSize="20dp"
       android:gravity="center"
       android:textStyle="bold"
       android:text="Cat List"
       android:textColor="#110C0C"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"/>
  </LinearLayout>
   <LinearLayout
       android:layout_marginLeft="20dp"
       android:id="@+id/linearHome2"
       android:layout_toRightOf="@+id/linearHome1"
       android:orientation="vertical"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:id="@+id/cv_catAdd"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="5dp"
        app:cardPreventCornerOverlap="true"
        app:cardElevation="5dp"
        android:layout_width="150dp"
        android:layout_height="150dp">

     <ImageView
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:src="@drawable/tombol_tambah"/>

    </androidx.cardview.widget.CardView>
    <TextView
        android:textSize="20dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="Add Cat"
        android:textColor="#110C0C"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
   </LinearLayout>
   <LinearLayout
       android:id="@+id/linearHome3"
       android:layout_below="@+id/linearHome1"
       android:orientation="vertical"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:id="@+id/cv_catType"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="5dp"
        app:cardPreventCornerOverlap="true"
        app:cardElevation="5dp"
        android:layout_width="150dp"
        android:layout_height="150dp">

     <ImageView
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:src="@drawable/ic_seleksi"/>

    </androidx.cardview.widget.CardView>
    <TextView
        android:textSize="20dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="Number Of Cat"
        android:textColor="#110C0C"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
   </LinearLayout>
   <LinearLayout
       android:layout_marginLeft="20dp"
       android:id="@+id/linearHome4"
       android:layout_below="@+id/linearHome1"
       android:layout_toRightOf="@+id/linearHome3"
       android:orientation="vertical"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:id="@+id/cv_catFood"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="5dp"
        app:cardPreventCornerOverlap="true"
        app:cardElevation="5dp"
        android:layout_width="150dp"
        android:layout_height="150dp">

     <ImageView
         android:layout_width="100dp"
         android:layout_height="100dp"
         android:layout_gravity="center"
         android:src="@drawable/ic_btn_food"/>

    </androidx.cardview.widget.CardView>
    <TextView
        android:textSize="18dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="Amount of Food"
        android:textColor="#110C0C"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
   </LinearLayout>
   <LinearLayout
       android:id="@+id/linearHome5"
       android:layout_below="@+id/linearHome3"
       android:orientation="vertical"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:id="@+id/cv_catTop"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="5dp"
        app:cardPreventCornerOverlap="true"
        app:cardElevation="5dp"
        android:layout_width="150dp"
        android:layout_height="150dp">

     <ImageView
         android:layout_gravity="center"
         android:layout_width="170dp"
         android:layout_height="170dp"
         android:src="@drawable/ic_btn_top"/>

    </androidx.cardview.widget.CardView>
    <TextView
        android:textSize="20dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="Top 5 Food"
        android:textColor="#110C0C"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
   </LinearLayout>
   <LinearLayout
       android:id="@+id/linearHome6"
       android:layout_toRightOf="@+id/linearHome5"
       android:layout_marginLeft="20dp"
       android:layout_below="@+id/linearHome4"
       android:orientation="vertical"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:id="@+id/cv_Logout"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="5dp"
        app:cardPreventCornerOverlap="true"
        app:cardElevation="5dp"
        android:layout_width="150dp"
        android:layout_height="150dp">

     <ImageView
         android:layout_width="110dp"
         android:layout_height="110dp"
         android:layout_gravity="center"
         android:src="@drawable/ic_btn_logout"/>

    </androidx.cardview.widget.CardView>
    <TextView
        android:textSize="20dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="Logout"
        android:textColor="#110C0C"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
   </LinearLayout>
  </RelativeLayout>
 </RelativeLayout>

</RelativeLayout>